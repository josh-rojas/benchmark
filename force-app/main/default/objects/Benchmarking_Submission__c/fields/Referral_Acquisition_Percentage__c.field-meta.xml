<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Referral_Acquisition_Percentage__c</fullName>
    <description>Percentage of new clients acquired through referrals - automatically calculated</description>
    <externalId>false</externalId>
    <formula>IF(Number_of_New_Clients__c > 0, (Number_of_Client_Referrals__c / Number_of_New_Clients__c) * 100, null)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Referral Acquisition Percentage</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>