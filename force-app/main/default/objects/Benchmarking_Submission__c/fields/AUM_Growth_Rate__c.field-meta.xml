<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>AUM_Growth_Rate__c</fullName>
    <description>Year-over-year AUM growth rate</description>
    <externalId>false</externalId>
    <formula>IF( AUM_PreviousYear__c > 0, ((AUM__c - AUM_PreviousYear__c) / AUM_PreviousYear__c) * 100, null )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>AUM Growth Rate</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField> 