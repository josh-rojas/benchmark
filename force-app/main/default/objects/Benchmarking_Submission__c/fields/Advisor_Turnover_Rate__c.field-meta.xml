<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Advisor_Turnover_Rate__c</fullName>
    <description>Annual advisor turnover rate - automatically calculated</description>
    <externalId>false</externalId>
    <formula>IF(Number_of_Advisors__c > 0, (Number_of_Advisors_Left__c / Number_of_Advisors__c) * 100, null)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Advisor Turnover Rate</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>