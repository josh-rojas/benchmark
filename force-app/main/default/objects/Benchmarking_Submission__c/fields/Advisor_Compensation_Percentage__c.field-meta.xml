<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Advisor_Compensation_Percentage__c</fullName>
    <description>Advisor compensation as a percentage of total revenue - automatically calculated</description>
    <externalId>false</externalId>
    <formula>IF(Total_Revenue__c > 0, (Total_Advisor_Compensation__c / Total_Revenue__c) * 100, null)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Advisor Compensation Percentage</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>