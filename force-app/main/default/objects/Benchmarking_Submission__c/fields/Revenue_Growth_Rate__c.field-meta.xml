<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Revenue_Growth_Rate__c</fullName>
    <description>Year-over-year revenue growth rate</description>
    <externalId>false</externalId>
    <formula>IF( Revenue_Previous_Year__c > 0, ((Total_Revenue__c - Revenue_Previous_Year__c) / Revenue_Previous_Year__c) * 100, null )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Revenue Growth Rate</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField> 