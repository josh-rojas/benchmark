<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Client_Growth_Rate__c</fullName>
    <description>Year-over-year client growth rate</description>
    <externalId>false</externalId>
    <formula>IF( Number_of_Clients_PreviousYear__c > 0, ((Number_of_Clients__c - Number_of_Clients_PreviousYear__c) / Number_of_Clients_PreviousYear__c) * 100, null )</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Client Growth Rate</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>