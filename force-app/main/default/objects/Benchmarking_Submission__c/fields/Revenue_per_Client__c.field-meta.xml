<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Revenue_per_Client__c</fullName>
    <description>Average revenue per client - automatically calculated</description>
    <externalId>false</externalId>
    <formula>IF(Number_of_Clients__c > 0, Total_Revenue__c / Number_of_Clients__c, null)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Revenue per Client</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Currency</type>
</CustomField>