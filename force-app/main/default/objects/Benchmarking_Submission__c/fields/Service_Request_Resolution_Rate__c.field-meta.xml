<?xml version="1.0" encoding="UTF-8"?>
<CustomField xmlns="http://soap.sforce.com/2006/04/metadata">
    <fullName>Service_Request_Resolution_Rate__c</fullName>
    <description>Percentage of service requests that were resolved - automatically calculated</description>
    <externalId>false</externalId>
    <formula>IF(Number_of_Service_Requests__c > 0, (Number_of_Resolved_Service_Requests__c / Number_of_Service_Requests__c) * 100, null)</formula>
    <formulaTreatBlanksAs>BlankAsZero</formulaTreatBlanksAs>
    <label>Service Request Resolution Rate</label>
    <precision>18</precision>
    <required>false</required>
    <scale>2</scale>
    <trackTrending>false</trackTrending>
    <type>Percent</type>
</CustomField>