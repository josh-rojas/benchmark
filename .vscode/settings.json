{
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/.sfdx": true
  },
  "files.exclude": {
    "**/.git": true,
    "**/.svn": true,
    "**/.hg": true,
    "**/CVS": true,
    "**/.DS_Store": true,
    "**/.sfdx": true,
    "**/node_modules": true,
    "**/.prettier*": true,
    "**/.eslint*": true,
    "**/.husky": true,
    "**/.github": false,
    "**/coverage": true,
    "**/.nyc_output": true,
    "**/*.tsbuildinfo": true,
    "**/.vscode-test": true
  },
  "[apex]": {
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "editor.detectIndentation": false,
    "editor.tabSize": 2
  },
  "[javascript]": {
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true,
    "editor.detectIndentation": false,
    "editor.tabSize": 2
  },
  "[xml]": {
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true
  },
  "[json]": {
    "editor.formatOnSave": true,
    "editor.formatOnPaste": true
  },
  "salesforce.einsteinForDevelopers.advanced": {
    "enableEinsteinAnalytics": true
  },
  "workbench.colorCustomizations": {}
}
